    "gatsby-plugin-purgecss": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sentry": "^1.0.0",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-plugin-sitemap": "^2.0.4",
    "gatsby-plugin-twitter": "^2.0.8",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.4",
    "gatsby-plugin-zopfli": "^1.0.2",
