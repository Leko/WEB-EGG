{
  "name": "webegg",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Leko <leko.noor@gmail.com>",
  "workspaces": [
    "./plugins/*"
  ],
  "browserslist": [
    "defaults"
  ],
  "resolutions": {
    "react-hot-loader": "~4.13.0"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "algoliasearch": "^4.10.5",
    "gatsby": "^3.14.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-algolia": "~0.24.0",
    "gatsby-plugin-brotli": "^2.0.0",
    "gatsby-plugin-canonical-urls": "^3.14.0",
    "gatsby-plugin-feed": "^3.14.0",
    "gatsby-plugin-google-tagmanager": "^3.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-nprogress": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-twitter": "^3.14.0",
    "gatsby-plugin-typography": "^3.14.0",
    "gatsby-plugin-zopfli": "^2.0.0",
    "gatsby-remark-autolink-headers": "^4.11.0",
    "gatsby-remark-copy-linked-files": "^4.11.0",
    "gatsby-remark-emoji-unicode": "^0.1.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-remark-responsive-iframe": "^4.11.0",
    "gatsby-remark-smartypants": "^4.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-instantsearch-dom": "^6.12.1",
    "react-typography": "^0.16.20",
    "typography": "^0.16.20",
    "typography-theme-wordpress-2016": "^0.16.19",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@proofdict/textlint-rule-proofdict": "^3.1.2",
    "babel-eslint": "^10.0.1",
    "dotenv": "^8.0.0",
    "eslint": "~5.16.0",
    "eslint-plugin-react": "~7.18.1",
    "husky": "~4.1.0",
    "image-size": "^0.8.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~1.19.0",
    "scaffdog": "^0.2.0",
    "textlint": "~11.7.6",
    "textlint-rule-date-weekday-mismatch": "~1.0.5",
    "textlint-rule-ja-hiragana-fukushi": "~1.2.0",
    "textlint-rule-ja-no-abusage": "~2.0.1",
    "textlint-rule-ja-no-redundant-expression": "~3.0.2",
    "textlint-rule-max-kanji-continuous-len": "~1.1.1",
    "textlint-rule-no-double-negative-ja": "~1.0.6",
    "textlint-rule-no-doubled-conjunction": "~1.0.3",
    "textlint-rule-no-doubled-conjunctive-particle-ga": "~1.1.1",
    "textlint-rule-no-dropping-the-ra": "~1.1.3",
    "textlint-rule-no-mix-dearu-desumasu": "~4.0.1",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-alphabet": "~1.0.1",
    "textlint-rule-no-nfd": "~1.0.2",
    "textlint-rule-no-todo": "~2.0.1",
    "textlint-rule-period-in-list-item": "~0.3.2",
    "textlint-rule-spellcheck-tech-word": "~5.0.0"
  },
  "license": "MIT",
  "repository": "Leko/WEB-EGG",
  "scripts": {
    "new-article": "scaffdog generate new-article",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx --ignore-pattern public 'gatsby*.js' 'src/**/*.js' 'plugins/*/*.js'",
    "lint:md": "textlint --cache --cache-location .cache/.textlintcache -f pretty-error 'content/blog/**/*.md'",
    "test": "exit 0",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "start": "DOTENV_CONFIG_PATH=.env.development node -r dotenv/config $(npm bin)/gatsby develop",
    "build": "gatsby build --verbose",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  }
}
