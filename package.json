{
  "name": "webegg",
  "version": "1.0.0",
  "description": "まだまだひよこ。サバクラ問わずwebに関連することを書き留めています",
  "main": "index.js",
  "scripts": {
    "postinstall": "flow-typed install",
    "test": "{TODO}",
    "lint:flow": "flow --show-all-errors",
    "lint:md": "textlint --cache --cache-location .caches/.textlintcache -f pretty-error source/post/*.md",
    "lint:md:fix": "textlint --fix source/post/*.md",
    "develop": "webpack --config ./webpack.config.js --watch -d --progress --colors",
    "build": "webpack --config ./webpack.config.js --bail -p",
    "build:cache": "sw-precache --config sw-precache-config.js",
    "deploy": "netlify -t $NETLIFY_ACCESS_TOKEN deploy -s $NETLIFY_SITE_ID"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Leko/WEB-EGG.git"
  },
  "keywords": [
    "blog",
    "leko",
    "WEB",
    "EGG"
  ],
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leko/WEB-EGG/issues"
  },
  "homepage": "https://github.com/Leko/WEB-EGG#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "algoliasearch": "^3.30.0",
    "almin": "^0.17.0",
    "classnames": "^2.2.5",
    "intersection-observer": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "shallow-equal-object": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "almin-logger": "^6.1.5",
    "babel-loader": "^8.0.4",
    "dotenv": "^6.0.0",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "husky": "^1.1.2",
    "netlify-cli": "^2.0.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "sw-precache": "^5.2.0",
    "textlint": "^11.0.0",
    "textlint-rule-date-weekday-mismatch": "^1.0.5",
    "textlint-rule-ja-hiragana-fukushi": "^1.0.3",
